<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebAppMvcClientLocation</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/WebAppMvcClientLocation.styles.css" asp-append-version="true" />
</head>
<body class="startPage" >
    <header>
        <div class="navBar">
        <ul class="">
            <li><a asp-controller="Home" asp-action="Index">Home</a></li>
            <li><a asp-controller="Clients" asp-action="Index"> Clients</a></li>
            <li><a href="\Locations">Locations</a></li>
            <li><a href="\Overview">Overview</a></li>
        </ul>
    </div>
    </header>
    <div class=" text-center">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - WebAppMvcClientLocation - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
